//https://codeforces.com/problemset/problem/1476/A

#include<bits/stdc++.h>
 
using namespace std;
using ll = long long int;
const ll MOD = 1e9+7;
 
void solve()
{  
    int n,k; cin>> n >> k;
    
    if(n==k){
        cout << 1;
        return;
    }
    if(n > k){
        if(n%k == 0){
            cout << 1;
        }
        else{
            cout << 2;
        }
        return;
    }

    int l = 1; int r = k;
    int ans = k;
    while(l <= r){
        int mid = (r+l)/2;
        if(mid*n >= k){
            ans = mid;
            r = mid-1;
        }
        else{
            l = mid+1; 
        }
    }

    cout << ans;
    return;
}
 
 
int main()
{   
    ios_base :: sync_with_stdio(0); cin.tie(0);
 
    ll t;
    cin >> t;
    while (t--)
    {
        solve();
        cout << "\n";
    }
    return 0;
}
